{
  "name": "ordiss-playwright-framework",
  "version": "1.0.0",
  "description": "Enterprise Playwright Testing Framework for ORDISS Application",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:smoke": "playwright test --grep @smoke",
    "test:regression": "playwright test --grep @regression",
    "test:login": "playwright test tests/login.spec.js",
    "test:unit-types": "playwright test tests/unit-types.spec.js",
    "test:parallel": "playwright test --workers=4",
    "test:headless": "playwright test --headed=false",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen https://10.10.10.10:700/login",
    "install:browsers": "playwright install",
    "install:deps": "playwright install-deps",
    "help": "echo 'Available commands: test, test:headed, test:debug, test:ui, test:smoke, test:regression, test:login, test:unit-types, test:parallel, test:headless, test:report, test:codegen, install:browsers, install:deps'",
    "run:custom": "node scripts/run-tests.js"
  },
  "keywords": [
    "playwright",
    "testing",
    "automation",
    "ordiss",
    "enterprise"
  ],
  "author": "ORDISS QA Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "faker": "^5.5.3",
    "dotenv": "^16.3.1"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "moment": "^2.29.4"
  }
}